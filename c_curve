import turtle
def curve_C(n,length):
    if n==0:
        turtle.forward(length)
    else:
        curve_C(n-1,length)
        turtle.left(90)
        curve_C(n-1,length)
        turtle.left(-90)

turtle.Screen().clear()
turtle.speed(0)
turtle.pensize(2)

n=9
length=10
turtle.penup()
turtle.goto(-190,-160)
turtle.pendown()
for i in range(1):
    curve_C(n,length)
turtle.done()
